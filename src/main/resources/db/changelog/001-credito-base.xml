        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="001-credito-base" author="liquibase">
        <createTable tableName="credito">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>

            <column name="numero_credito" type="VARCHAR(50)"/>
            <column name="numero_nfse" type="VARCHAR(50)"/>
            <column name="data_constituicao" type="DATE"/>
            <column name="valor_issqn" type="DECIMAL(15,2)"/>
            <column name="tipo_credito" type="VARCHAR(50)"/>
            <column name="simples_nacional" type="BOOLEAN"/>
            <column name="aliquota" type="DECIMAL(5,2)"/>
            <column name="valor_faturado" type="DECIMAL(15,2)"/>
            <column name="valor_deducao" type="DECIMAL(15,2)"/>
            <column name="base_calculo" type="DECIMAL(15,2)"/>

            <column name="status" type="VARCHAR(20)"/>
            <column name="data_solicitacao" type="TIMESTAMP"/>
            <column name="solicitado_por" type="VARCHAR(50)"/>
            <column name="aprovado_por" type="VARCHAR(50)"/>
            <column name="comentario_analise" type="TEXT"/>
            <column name="data_analise" type="TIMESTAMP"/>
        </createTable>

        <createIndex indexName="idx_credito_numero_nfse" tableName="credito">
            <column name="numero_nfse"/>
        </createIndex>

        <createIndex indexName="idx_credito_numero_credito" tableName="credito">
            <column name="numero_credito"/>
        </createIndex>

